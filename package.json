{
  "name": "@aurora-is-near/relayer",
  "version": "0.0.0",
  "description": "NEAR Ethereum JSON RPC interface",
  "type": "module",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "AUTHORS",
    "CHANGES.md",
    "LICENSE",
    "README.md",
    "VERSION",
    "package.json",
    "lib/index.js",
    "lib/index.d.ts",
    "lib/app.js",
    "lib/app.d.ts",
    "lib/errors.js",
    "lib/errors.d.ts",
    "lib/eip-712-helpers.js",
    "lib/eip-712-helpers.d.ts"
  ],
  "bin": {
    "aurora-relayer": "lib/index.js"
  },
  "scripts": {
    "start": "nodemon",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint test && eslint **/*.ts"
  },
  "keywords": [
    "Aurora",
    "Ethereum",
    "NEAR",
    "Web3"
  ],
  "author": "aurora.dev",
  "authors": [
    "Arto Bendiken <arto@near.org> (https://ar.to)",
    "Illia Polosukhin <ilblackdragon@gmail.com> (https://github.com/ilblackdragon)"
  ],
  "license": "CC0-1.0",
  "homepage": "https://github.com/aurora-is-near/aurora-relayer",
  "bugs": "https://github.com/aurora-is-near/aurora-relayer/issues",
  "repository": "https://github.com/aurora-is-near/aurora-relayer.git",
  "dependencies": {
    "@aurora-is-near/engine": "git://github.com/aurora-is-near/aurora.js#e25e60a",
    "commander": "^7.2.0",
    "cors": "^2.8.5",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^7.0.10",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "jest": "^26.6.3",
    "near-web3-provider": "https://github.com/aurora-is-near/near-web3-provider",
    "rlp": "^2.2.6",
    "ts-jest": "^26.5.5"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cram": "^0.8.3",
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testTimeout": 30000,
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
